      <generator class="native">
        <param name="sequence">METADISTRIBUTE_DISTRIBUTE_ID</param>
      </generator>
    </id>
    <property name="OnlineInformation" column="ONLINE_INFORMATION" type="string" />
    <property name="Relation" column="RELATION" type="string" />
