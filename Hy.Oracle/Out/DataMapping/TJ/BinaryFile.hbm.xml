      <generator class="native">
        <param name="sequence">BINARY_FILE_FILE_ID</param>
      </generator>
    </id>
    <property name="ProjectId" column="PROJECT_ID" type="string" />
    <property name="FileType" column="FILE_TYPE" type="string" />
    <property name="FileName" column="FILE_NAME" type="string" />
    <property name="Remark" column="REMARK" type="string" />
    <property name="FilergnPath" column="FILERGN_PATH" type="string" />
    <property name="FileLength" column="FILE_LENGTH" type="Decimal" />
    <property name="RgnId" column="RGN_ID" type="Decimal" />
