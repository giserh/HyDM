	<class name="Frame.Define.ClassInfo,Frame" table="T_Sys_ClassInfo" lazy="false" >
		<id name="ID" column="ID" type="string">
			<generator class="uuid.hex"/>
		</id>
		<property column="ClassName" type="String" name="ClassName"/>
